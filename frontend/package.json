    "server:start": "json-server ./db.json --port=3001 --watch",
    "deploy": "rm -rf /home/mk/stuff/full-stack-open-be/static && mkdir /home/mk/stuff/full-stack-open-be/static  && cp -r dist/* /home/mk/stuff/full-stack-open-be/static"
  },
